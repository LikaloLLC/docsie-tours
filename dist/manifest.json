{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "storage",
    "tabs",
    "debugger",
    "activeTab",
    "cookies",
    "http://*/",
    "https://*/",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "index.html"
  ],
  
"externally_connectable": {
  "ids": [
    "*"
  ],
  "matches": [
    "http://ec2-54-224-135-131.compute-1.amazonaws.com:8003/*"
  ],
  "accepts_tls_channel_id": false
},
  "content_security_policy": "script-src 'self' 'sha256-CMyYic0d7L0Q9AwjGU0n6buHFRR6bU3TOAe0P7DEJrk='; object-src 'self'; frame-src 'self'; default-src 'self';connect-src http://ec2-54-224-135-131.compute-1.amazonaws.com:8003/"
}